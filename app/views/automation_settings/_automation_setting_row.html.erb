<tr id="automation_setting_row_<%= automation_setting.id %>">
  <td><%= check_box_tag '', automation_setting.id, false, { class: 'automation_setting_checkbox' } %></td>
  <td class="text-ellipsis">
    <%= link_to automation_setting.name, automation_setting_path(automation_setting.id), style: 'color: #1b00ff;', data: { turbolinks: false } %>
  </td>
  <td class="text-ellipsis">
    <%= automation_setting.sports_name %>
  </td>
  <td class="text-ellipsis">
    <%= automation_setting.facility_names.join('、') %>
  </td>
  <td>
    <%= link_to '処理開始', 'javascript:void(0);', onclick: "openOsakaCityOpas(#{automation_setting.sports_type}, \"#{automation_setting.facility_types}\")", class: "btn btn-primary btn-sm" %>
  </td>
  <td>
    <div class="table-actions">
      <%= link_to automation_setting_path(automation_setting.id), "data-color": "#e95959", "data-toggle": "tooltip", "data-placement": "top", "title": "詳細", data: { turbolinks: false } do %>
        <i class="icon-copy fa fa-file-text-o" aria-hidden="true"></i>
      <% end %>
      <%= link_to edit_automation_setting_path(automation_setting.id), "data-color": "#265ed7", "data-toggle": "tooltip", "data-placement": "top", "title": "編集", data: { turbolinks: false } do %>
        <i class="icon-copy dw dw-edit2"></i>
      <% end %>
    </div>
  </td>
</tr>
