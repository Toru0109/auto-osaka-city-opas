$("#delete-link-<%=@automation_setting.id%>").on('ajax:success', function() {
  const row = document.getElementById("automation_setting_row_<%= @automation_setting.id %>");
  const tbody = row.closest('tbody[id="automation_settings_tbody"]')

  row.remove();
  if (tbody.childElementCount <= 0) {
    const insertRow = document.getElementById('no-data-row');
    tbody.appendChild(insertRow);
  }
});
