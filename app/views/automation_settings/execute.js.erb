if (<%= response.status %> === 202) {
  Swal.fire({
    icon: 'success',
    text: '処理を開始中です。ブラウザを開いたままお待ちください。',
    showConfirmButton: false,
    timer: 2000
  })
}
if (<%= response.status %> === 404) {
  Swal.fire({
    icon: 'error',
    text: '処理の開始に失敗しました。'
  })
}
if (<%= response.status %> >= 500) {
  Swal.fire({
    icon: 'error',
    text: '予期せぬエラーが発生しました。'
  })
}
